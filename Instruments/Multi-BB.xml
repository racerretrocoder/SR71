 <animation>
        <type>select</type>
        <object-name>descend</object-name>
        <condition>
        <less-than>
       <property>autopilot/internal/vert-speed-fpm</property>
        <value>0</value>
        </less-than>
        </condition>
</animation>

 <animation>
        <type>select</type>
        <object-name>ascend</object-name>
        <condition>
        <greater-than>
       <property>autopilot/internal/vert-speed-fpm</property>
        <value>0</value>
        </greater-than>
        </condition>
</animation>


 <animation>
