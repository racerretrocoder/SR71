<PropertyList>

    <path>Models/screen.ac</path>
    <texture-path>Models/Textures</texture-path>
    <model>
        <name>PanelV</name>
        <path>Aircraft/SR71/Models/AI.xml</path>
        <offsets>
  <x-m>-1.70</x-m>
   <y-m>-0.25</y-m>
   <z-m>0.1</z-m>

        </offsets>
    </model>


 <animation>
  <type>pick</type>
   <object-name>ap</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.ap();</script>
    </binding>
   </action>
 </animation>


 <animation>
  <type>pick</type>
   <object-name>autoorbit</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.orbitauto();</script>
    </binding>
   </action>
 </animation>

 <animation>
  <type>pick</type>
   <object-name>prograde</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.prograde();</script>
    </binding>
   </action>
 </animation>


  <animation>
  <type>pick</type>
   <object-name>retrograde</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.retrograde();</script>
    </binding>
   </action>
 </animation>

  <animation>
  <type>pick</type>
   <object-name>left</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.left();</script>
    </binding>
   </action>
 </animation>
  <animation>
  <type>pick</type>
   <object-name>right</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.right();</script>
    </binding>
   </action>
 </animation>

  <animation>
  <type>pick</type>
   <object-name>down</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.dn();</script>
    </binding>
   </action>
 </animation>


  <animation>
  <type>pick</type>
   <object-name>up</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
  <command>nasal</command>
  <script>screen.up();</script>
    </binding>
   </action>
 </animation>


 <animation>
  <type>material</type>
            <object-name>ap</object-name>
            <object-name>pe</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.8</green>
    <blue>1.0</blue>
  </ambient>
  <diffuse>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.1</blue>
  </diffuse>
  <emission>
    <red>0.3</red>
    <green>0.5</green>
    <blue>0.8</blue>
  </emission> 
 </animation>


