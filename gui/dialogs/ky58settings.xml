<?xml version="1.0"?>
<PropertyList>
  <name>ky58settings</name>
  <layout>vbox</layout>

  <text>
    <label>KY-58 Secure MPChat Controls</label>
  </text>

  <hrule/>

  <group>
    <layout>vbox</layout>
    <halign>center</halign>

    <text>
      <label>Knobs</label>
    </text>

    <checkbox>
      <halign>left</halign>
      <label>MASTER ARM</label>
      <property>controls/ky58/diagmst</property>
      <binding>
        <command>property-toggle</command>
        <property>controls/ky58/diagmst</property>
      </binding>
    </checkbox>


    <button>
      <pref-width>86</pref-width>
      <pref-height>25</pref-height>
      <legend>ON</legend>
	  	<halign>left</halign>
      <border>2</border>
      <binding>
<command>nasal</command>
            <script>
			  			   setprop("controls/ky58/main",1);
			</script>
      </binding>
            	<enable>
                <property>controls/ky58/diagmst</property>
        </enable>
    </button>
    <button>
